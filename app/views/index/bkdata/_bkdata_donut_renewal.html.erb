<script>
var donut_auto_renew;
function init_donut_graph_auto_renew(){
  console.log("init_donut_graph called");
  $.ajax({
      url : "/admin/transaction_is_auto_renew",
      type : "post",
      success : function(data){
        console.log(data);
        donut_auto_renew = Morris.Donut({
            element: 'morris-donut-chart-renewal',
            data: [{
                label: "None Auto",
                value: data[0].a
              }, {
                label: "Auto Renewal",
                value: data[1].a
              }],
            resize: true
        });
      }
  })
};

$(function() {
  init_donut_graph_auto_renew();
});

</script>
